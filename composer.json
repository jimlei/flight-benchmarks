{
    "repositories": [
      {
            "type": "package",
            "package": {
                "name": "gabor/redbean",
                "version": "4.0.5",
                "dist": {
                    "url": "http://www.redbeanphp.com/downloadredbean.php",
                    "type": "tar"
                }
            }
        }

    ],
    
    "require": {
        "doctrine/dbal": "2.5.*@dev",
        "doctrine/orm": "2.5.*@dev",
        "imagine/imagine": "dev-master",
        "mikecao/flight": "dev-master",
        "monolog/monolog": "1.10.*@dev",
        "symfony/yaml": "2.*",
        "twig/twig": "dev-master",
        "gabor/redbean": "4.0.*"
    },
    "autoload": {
        "psr-0": {
            "": "app/doctrine/entities/"
            },
        "files": [
                "vendor/gabor/redbean/rb.php"
        ]
    },
    "minimum-stability": "dev"
}